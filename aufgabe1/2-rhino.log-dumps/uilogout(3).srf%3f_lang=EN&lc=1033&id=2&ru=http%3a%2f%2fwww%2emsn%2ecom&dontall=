<script language="javascript">
var iTotalImage = 0;
var ids = new Array('2','3');
    if (ids.length == 0) {
        TimeoutReturn();
    }
var idLoggedIn = new Array(ids.length);
var badImageSrc = "http://www.passportimages.com/1033/signout_bad.gif";
